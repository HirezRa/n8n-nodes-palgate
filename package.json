{
	"name": "n8n-nodes-palgate",
	"version": "1.0.16",
	"description": "n8n community node for PAL Portal API - manage users, cars, places, devices, and organizations",
	"license": "MIT",
	"homepage": "https://github.com/pal-es/n8n-nodes-palgate",
	"keywords": [
		"n8n-community-node-package",
		"pal-portal",
		"pal-es",
		"device-management",
		"user-management"
	],
	"author": {
		"name": "PAL Portal Team",
		"email": "support@pal-es.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/HirezRa/n8n-nodes-palgate.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"release:patch": "bash release.sh patch || powershell -ExecutionPolicy Bypass -File release.ps1 patch",
		"release:minor": "bash release.sh minor || powershell -ExecutionPolicy Bypass -File release.ps1 minor",
		"release:major": "bash release.sh major || powershell -ExecutionPolicy Bypass -File release.ps1 major"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/PalGateApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/PalGate/PalGate.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "^2.2.2"
	},
	"overrides": {
		"form-data": "^4.0.4"
	}
}
